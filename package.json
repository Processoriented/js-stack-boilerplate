{
  "name": "js-stack-from-scratch",
  "version": "2.4.5",
  "repository": "Processoriented/js-stack-boilerplate",
  "author": "Vincent Engler - @processoriented",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "npm run lint && flow && jest --coverage",
    "precommit": "npm run test",
    "prepush": "npm run test && npm run prod:build"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.0.0",
    "compression": "^1.6.2",
    "express": "^4.15.0",
    "immutable": "^4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "popper.js": "^1.13.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-jss": "^8.2.1",
    "react-redux": "^5.0.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^14.0.4",
    "babel-preset-env": "^1.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "fetch-mock": "^6.0.0",
    "flow-bin": "^0.64.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.2",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.1"
  }
}
